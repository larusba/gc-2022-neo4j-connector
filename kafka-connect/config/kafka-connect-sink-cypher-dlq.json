{
    "topics": "movie",
    "connector.class": "streams.kafka.connect.sink.Neo4jSinkConnector",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "key.converter.schemas.enable": false,
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": false,
    "kafka.bootstrap.servers": "broker:9093",
    "errors.tolerance": "all",
    "errors.log.enable": true,
    "errors.deadletterqueue.topic.name": "dlq-movie",
    "errors.log.include.messages": true,
    "errors.deadletterqueue.topic.replication.factor": 1,
    "neo4j.server.uri": "bolt://neo4j:7687",
    "neo4j.authentication.basic.username": "neo4j",
    "neo4j.authentication.basic.password": "password",
    "neo4j.encryption.enabled": false,
    "neo4j.topic.cypher.movie": "MERGE (m:Movie {title: event.title, released: event.released, tagline: event.tagline, timestamp: timestamp()}) RETURN m"
}